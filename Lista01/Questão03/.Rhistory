)
thread = thread %>%
filter(time < 75) %>%
mutate(threads = row_number())
ggplot(data=thread, aes(x=time)) +
geom_histogram()
thread = thread %>%
mutate(cumulative = cumsum(time))
thread
ggplotly(
ggplot(data=time, aes(x=threads, y=time)) +
geom_line()
)
ggplotly(
ggplot(data=time, aes(x=threads, y=cumulative)) +
geom_line()
)
thread = thread %>%
filter(time < 90) %>%
mutate(threads = row_number())
ggplotly(
ggplot(data=thread, aes(x=number, y=time)) +
geom_line()
)
thread = thread %>%
mutate(cumulative = cumsum(time))
thread
ggplotly(
ggplot(data=thread, aes(x=threads, y=time)) +
geom_line()
)
ggplotly(
ggplot(data=thread, aes(x=threads, y=cumulative)) +
geom_line()
