library(tidyverse)
library(dplyr)
library(plotly)
start = read.csv(("start.csv"), header=FALSE)
colnames(start) <- c("PID","sequence","start")
end = read.csv(("end.csv"), header=FALSE)
colnames(end) <- c("PID","sequence","end")
time = merge(start, end, by=c("PID", "sequence")) %>%
mutate(time = end-start) %>%
select(c("PID","sequence","time"))
time
ggplotly(
ggplot(data=time, aes(x=sequence, y=time)) +
geom_line()
)
time = time %>%
filter(time < 5000) %>%
mutate(threads = row_number())
time = time %>%
mutate(cumulative = cumsum(time))
time
ggplotly(
ggplot(data=time, aes(x=threads, y=time)) +
geom_line()
)
ggplotly(
ggplot(data=time, aes(x=threads, y=cumulative)) +
geom_line()
)
thread = read.csv(("logThread.csv"), header=FALSE)
colnames(thread) <- c("number", "time")
thread
ggplotly(
ggplot(data=thread, aes(x=number, y=time)) +
